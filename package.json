{
  "name": "gatsby-static-site",
  "version": "1.0.0",
  "private": true,
  "description": "gatsby-static-site",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "dev": "gatsby develop -p 8081",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy-ci": "gatsby-plugin-s3 deploy --yes && aws cloudfront create-invalidation --distribution-id $DISTRIBUTION --paths \"/*\""
  },
  "dependencies": {
    "aws-sdk": "^2.1395.0",
    "dotenv": "^16.1.4",
    "gatsby": "^5.10.0",
    "gatsby-plugin-env-variables": "^2.3.0",
    "gatsby-plugin-image": "^3.10.0",
    "gatsby-plugin-s3": "^0.4.1",
    "gatsby-source-contentful": "^8.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
